{
  "root": true,
  "env": {
    "jest/globals": true,
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:jest/all",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended",
    "prettier/babel"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "rules": {}
}